<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/prueba"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.michele.proyectojuegoparejas.MainActivity">

    <fragment android:name="com.example.michele.proyectojuegoparejas.FragmentMenu"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="40"
        android:id="@+id/menu_principal"
        tools:layout="@layout/fragment_menu"/>

    <FrameLayout
        android:id="@+id/contenedor_fragmentos_main"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="60"></FrameLayout>

</LinearLayout>
